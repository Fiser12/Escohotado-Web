{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev",
    "devturbo": "pnpm dev --turbo",
    "devsafe": "rm -rf .next && pnpm dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "ci": "payload migrate && pnpm build",
    "payload": "payload",
    "generate:types": "payload generate:types",
    "generate:db-schema": "payload generate:db-schema",
    "generate:schema": "payload-graphql generate:schema",
    "standalone-script": "payload run ./src/scripts/standalone-script.ts",
    "generate:importmap": "payload generate:importmap",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.741.0",
    "@payloadcms/db-postgres": "3.20.0",
    "@payloadcms/graphql": "3.20.0",
    "@payloadcms/live-preview-react": "3.20.0",
    "@payloadcms/next": "3.20.0",
    "@payloadcms/plugin-nested-docs": "3.20.0",
    "@payloadcms/plugin-search": "3.20.0",
    "@payloadcms/plugin-sentry": "3.20.0",
    "@payloadcms/plugin-stripe": "3.20.0",
    "@payloadcms/richtext-lexical": "3.20.0",
    "@payloadcms/richtext-slate": "3.20.0",
    "@payloadcms/storage-s3": "3.20.0",
    "@payloadcms/ui": "3.20.0",
    "@sentry/nextjs": "^8.54.0",
    "atropos": "^2.0.2",
    "babel-plugin-react-compiler": "19.0.0-beta-714736e-20250131",
    "classnames": "^2.5.1",
    "config": "workspace:*",
    "cross-env": "^7.0.3",
    "framer-motion": "^12.1.0",
    "gaudi": "workspace:*",
    "graphql": "^16.10.0",
    "hegel": "workspace:*",
    "jsonwebtoken": "^9.0.2",
    "next": "15.1.6",
    "next-auth": "5.0.0-beta.25",
    "nuqs": "^2.3.2",
    "payload": "3.20.0",
    "payload-authjs": "^0.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.33.5",
    "stripe": "17.6.0",
    "tailwind-merge": "^3.0.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@storybook/addon-designs": "^8.1.0",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-viewport": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/nextjs": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@tailwindcss/postcss": "4.0.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.1",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "eslint": "^9.19.0",
    "eslint-config-next": "15.1.6",
    "eslint-plugin-storybook": "^0.11.2",
