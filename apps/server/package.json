{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev",
    "devturbo": "pnpm dev --turbo",
    "devsafe": "rm -rf .next && pnpm dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "ci": "payload migrate && pnpm build",
    "payload": "payload",
    "generate:types": "payload generate:types",
    "generate:schema": "payload-graphql generate:schema",
    "standalone-script": "payload run ./src/scripts/standalone-script.ts",
    "generate:importmap": "payload generate:importmap",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.686.0",
    "@payloadcms/db-postgres": "3.0.0-beta.119",
    "@payloadcms/next": "3.0.0-beta.119",
    "@payloadcms/plugin-sentry": "3.0.0-beta.119",
    "@payloadcms/plugin-stripe": "3.0.0-beta.119",
    "@payloadcms/richtext-lexical": "3.0.0-beta.119",
    "@payloadcms/richtext-slate": "3.0.0-beta.119",
    "@payloadcms/storage-s3": "3.0.0-beta.119",
    "@payloadcms/ui": "3.0.0-beta.119",
    "@sentry/nextjs": "^8.37.1",
    "babel-plugin-react-compiler": "0.0.0-experimental-48eb8f4-20240822",
    "config": "workspace:*",
    "cross-env": "^7.0.3",
    "gaudi": "workspace:*",
    "graphql": "^16.8.2",
    "hegel": "workspace:*",
    "jsonwebtoken": "^9.0.2",
    "next": "15.0.1",
    "next-auth": "^5.0.0-beta.25",
    "payload": "3.0.0-beta.119",
    "payload-authjs": "^0.3.2",
    "react": "rc",
    "react-dom": "rc",
    "sharp": "^0.33.5",
    "stripe": "17.2.1",
    "tailwind-merge": "^2.5.4",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@storybook/addon-designs": "^8.0.4",
    "@storybook/addon-essentials": "^8.4.1",
    "@storybook/addon-interactions": "^8.4.1",
    "@storybook/addon-links": "^8.4.1",
    "@storybook/addon-viewport": "^8.4.1",
    "@storybook/blocks": "^8.4.1",
    "@storybook/nextjs": "^8.4.1",
    "@storybook/react": "^8.4.1",
    "@storybook/test": "^8.4.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.14.9",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-storybook": "^0.10.1",
    "postcss": "^8.4.47",
    "storybook": "^8.4.1",
    "tailwindcss": "^3.4.14",
    "typescript": "5.5.4"
  }
}
